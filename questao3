import java.util.Scanner;

class Main {
  public static void main(String[] args) {

    Scanner ler = new Scanner(System.in);

    double num1;
    double num2;
    double result;
    
    num1 = 0;
    System.out.printf("Digite um número:\t");
    num2 = ler.nextDouble();

    num1 = ((1 + 99)*99)/2;
    num2 = ((1 + 50)*50)/2;

    result = num1 / num2;

    System.out.printf("O resultado é:\t" + num1,num2,result);
    
  }
}

   

